{
  "name": "akamai",
  "version": "1.7.0",
  "description": "",
  "main": "built/main.js",
  "scripts": {
    "build": "rm -rf built/ && rollup -c && cd built && tar -czvf ../'ew_'$npm_package_config_ewid'_'$npm_package_version'.tgz' *",
    "upload": "akamai edgeworkers upload --bundle ./'ew_'$npm_package_config_ewid'_'$npm_package_version'.tgz' 90462",
    "activate": "akamai edgeworkers activate 90462 production",
    "status": "akamai edgeworkers status 90462",
    "curl": "curl http://statsig.com/helloworld --connect-to ::statsig.com.edgesuite.net -v -s -H 'Accept-Language: en'",
    "start-sandbox": "akamai sandbox start",
    "upload-sandbox": "akamai sandbox add-edgeworker 90462 ./'ew_'$npm_package_config_ewid'_'$npm_package_version'.tgz'",
    "update-sandbox": "akamai sandbox update-edgeworker 90462 ./'ew_'$npm_package_config_ewid'_'$npm_package_version'.tgz'",
    "curl-sandbox": "curl --header 'Host: statsig.com' http://127.0.0.1:9550/helloworld -v"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "statsig-node-lite": "^0.3.1",
    "tslib": "^2.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/akamai-edgeworkers": "^1.1.17",
    "rollup-plugin-copy-assets": "^2.0.3",
    "typescript": "^5.6.3"
  },
  "type": "module"
}